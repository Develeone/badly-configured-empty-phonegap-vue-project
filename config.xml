<?xml version='1.0' encoding='utf-8'?>
<widget id="su.awake.example_phonegap_project" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Example</name>
    <description>
        Example
    </description>
    <author email="contacts@awake.su" href="http://awake.su">
        Awake!
    </author>
    <content src="dist/index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="orientation" value="portrait" />
    <platform name="android">
        <icon density="mdpi" src="phonegap_www_default/res/icon/mipmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="phonegap_www_default/res/icon/mipmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="phonegap_www_default/res/icon/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="phonegap_www_default/res/icon/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="phonegap_www_default/res/icon/mipmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <access origin="*" />
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="whatsapp:*" />
    <allow-navigation href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
    <plugin name="cordova-plugin-battery-status" spec="~1.2.4" />
    <plugin name="cordova-plugin-camera" spec="~2.4.1" />
    <plugin name="cordova-plugin-media-capture" spec="~1.4.3" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.7" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.1" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.5" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
